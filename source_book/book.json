{
  "plugins": [
    "anchor-navigation-ex",
    "edit-link",
    "copy-code-button",
    "theme-comscore",
    "ga",
    "search-pro",  "-lunr", "-search", 
    "theme-faq",
    "theme-api", 
    "chapter-fold",  "expandable-chapters",
    "pageview-count" , "splitter", "custom-favicon ",
    "todo", "multipart", "insert-logo ",
    "back-to-top-button", "code", "github",

  ],
  "pluginsConfig": {
    "fontSettings": {
      "theme": "night",
      "family": "sans"
    },
    "theme-api": {
      "theme": "dark"
    },
    "theme-default": {
      "showLevel": false
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": false,
      "printLog": false,
      "multipleH1": false,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fa fa-navicon",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "edit-link": {
      "base": "https://github.com/a920604a/gitbook/edit/master/",
      "label": "Edit This Page"
    },
    "ga": {
      "token": "UA-159327826-2"
    }
  }
}