{
  "plugins": [
    "anchor-navigation-ex",
    "edit-link",
    "ga",
    "github",
    "copy-code-button",
    "theme-comscore",
    "katex",
     "-lunr", "-search", 
    "theme-api", 
    "chapter-fold",  "expandable-chapters",
    "pageview-count" , "splitter",
     "multipart", 
    "back-to-top-button", "code",
    "page-treeview"

  ],
  "pluginsConfig": {
    "fontSettings": {
      "theme": "night",
      "family": "sans"
    },
    "page-treeview": {
      "copyright": "Copyright &#169; aleen42",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "theme-api": {
      "theme": "dark"
    },
    "theme-default": {
      "showLevel": false
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": false,
      "printLog": false,
      "multipleH1": false,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fa fa-navicon",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "edit-link": {
      "base": "https://github.com/a920604a/gitbook/edit/master/",
      "label": "Edit This Page"
    },
    "ga": {
      "token": "UA-159327826-2"
    },
    "github": {
      "url": "https://github.com/a920604a"
  },
    "mathjax":{
      "forceSVG": true
  }
  }
}