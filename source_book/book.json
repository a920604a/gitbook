{
  "styles": {
    "website": "styles/main.css",
    "ebook": "styles/main.css",
    "pdf": "styles/main.css",
    "mobi": "styles/main.css",
    "epub": "styles/main.css"
  },
  "plugins": [
    "anchor-navigation-ex",
    "edit-link",
    "ga",
    "github",
    "theme-api",
    "copy-code-button",
    "theme-comscore",
    "splitter",
    "-lunr",
    "-search",
    "search-pro",
    "chapter-fold",
    "expandable-chapters",
    "pageview-count",
    "multipart",
    "back-to-top-button",
    "code",
    "mathjax",
    "advanced-emoji",
    "prism", "-highlight" ,
    "tbfed-pagefooter",
    "auto-scroll-table",
    "popup",

    "alerts","flexible-alerts"
  ],
  "pluginsConfig": {
    "fontSettings": {
      "theme": "night",
      "family": "sans"
    },
    "theme-default": {
      "showLevel": true
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": false,
      "printLog": false,
      "multipleH1": false,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fa fa-navicon",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "theme-api": {
      "theme": "dark"
    },
    "edit-link": {
      "base": "https://github.com/a920604a/gitbook/edit/master/",
      "label": "Edit This Page"
    },
    "ga": {
      "token": "UA-159327826-2"
    },
    "github": {
      "url": "https://github.com/a920604a"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ]
    },
    "tbfed-pagefooter": {
      "copyright":"",
      "modify_label": "該文件最後修改時間：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
  }
  }
}